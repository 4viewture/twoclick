{
	"name": "4viewture/twoclick",
	"description": "allows to wrap all content elements with 2 clicks",
	"type": "typo3-cms-extension",
	"license": [
		"GPL-2.0+"
	],
	"keywords": [
		"TYPO3",
		"extension"
	],
	"authors": [
		{
			"name": "Kay Strobach",
			"email": "hello@4viewture.de",
			"homepage": "https:\/\/www.4viewture.de",
			"role": "Developer"
		}
	],
	"homepage": "https:\/\/www.fucodo.de",
	"require": {
		"typo3\/cms-core": "^12.4",
		"b13/container": "^2.3.0"
	},
	"require-dev": {
		"phpunit\/phpunit": "^5.6"
	},
	"autoload": {
		"psr-4": {
			"FourViewture\\TwoClick\\": "Classes\/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\Core\\Tests\\": "..\/..\/typo3\/sysext\/core\/Tests\/",
			"TYPO3\\CMS\\Fluid\\Tests\\": "..\/..\/typo3\/sysext\/fluid\/Tests\/"
		}
	},
	"config": {
		"bin-dir": ".Build\/bin\/",
		"vendor-dir": ".Build\/vendor\/"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "twoclick"
		}
	}
}
